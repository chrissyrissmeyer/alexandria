<% if desc = @presenter.description %>
  <% content_for :head do %>
    <meta name="description" content="<%= desc %>">
  <% end %>
<% end %>

<% # overridden to remove multiple upload and show actions %>
<% provide :page_title, @presenter.page_title %>
<% provide :page_header do %>
  <h1><%= @presenter %></h1>
<% end %>

<div class="row">
  <div class="col-md-12">
    <%= render 'index_maps', presenter: @presenter %>
    <%= render 'component_maps', presenter: @presenter %>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <%= render 'attributes', presenter: @presenter %>
  </div>
</div>
